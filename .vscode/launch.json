    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: FastAPI",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "--factory",
                "--reload",
                "--host=0.0.0.0",
                "--log-config=config/debug-logger.yml",
                "--timeout-graceful-shutdown=10",
                "doc_label:init_app",
            ],
            "jinja": true,
            "justMyCode": false
        }
    ]
}
